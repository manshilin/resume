<main>
    {{> title data = titleEducation}}
    {{#with main}}
    <section>
        <ul>
            {{#each educations}}
            <li>
                {{name}}
                {{#unless isEnd}} (ще в процесі){{/unless}}
            </li>
            {{/each}}
        </ul>
    </section>
    {{/with}}
    {{> title data = titleCertificates}}
    {{#with main}}
    <section>
        <ul>
            {{#each certificates}}
            <li>
                {{name}} ({{id}})
            </li>
            {{/each}}
        </ul>
    </section>
    {{/with}}
</main>